{
    "agents": {
        "defaults": {
            "model": {
                "primary": "anthropic/claude-sonnet-4-5-20250929"
            }
        }
    },
    "gateway": {
        "port": 18789,
        "bind": "loopback",
        "mode": "local",
        "auth": {
            "mode": "token",
            "token": "GENERATE_WITH_openssl_rand_-hex_24"
        }
    },
    "channels": {
        "telegram": {
            "enabled": true,
            "botToken": "YOUR_TELEGRAM_BOT_TOKEN_HERE",
            "dmPolicy": "allowlist",
            "allowFrom": ["YOUR_TELEGRAM_USER_ID"]
        }
    },
    "tools": {
        "elevated": {
            "enabled": true
        }
    },
    "env": {
        "WP_PATH": "/var/www/html",
        "WP_SITE_URL": "http://YOUR_DOMAIN",
        "WP_APP_USER": "YOUR_WP_ADMIN_USER",
        "WP_APP_PASSWORD": "YOUR_WP_APP_PASSWORD"
    }
}
